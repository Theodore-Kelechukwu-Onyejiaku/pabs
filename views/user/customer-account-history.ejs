<%- include("../partials/user/header") %>

<!--/col-->

<div class="col main pt-5 mt-3 bg-light">
    <div class="title">
        <h4 class="display-5 d-sm-block"> 
            ACCOUNT HISTORY
        </h4>
    </div>

    <!-- add user form -->
    <div class="container users">

        <div class="table-responsive pt-3 pb-5">
            <table class="userTable table table-bordered table-stripped">
                <thead>
                    <tr>
                        <th>S/N</th>
                        <th>Date</th>
                        <th>Description</th>
                        <th>Ref</th>
                        <th>Withdrawals</th>
                        <th>Deposit</th>
                        <th>Balance</th>
                    </tr>
                </thead>
                <tbody>
                    <% var row = 0 %>
                    
                    <% row++ %>
                    <tr class="tableRow-" + row>
                        <td><%= row %></td>
                        <td><%=data.dateTwo%> </td>
                        <td><%=data.description%> </td>
                        <td><%= data.ref%></td>
                        <td><%= data.withdrawals%></td>
                        <td><%=data.deposit%> </td>
                        <td><%= data.account_balance%></td>
                    </tr>                                      
                </tbody>
            </table>
        </div>
    
    </div>

</div>
<!--/main col-->
</div>

</div>
<!--/.container-->


<%- include("../partials/user/footer") %>
