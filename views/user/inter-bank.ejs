	<script type="text/javascript">
        document.getElementById('otp_value').length;
    
        function comp() 
        {
            var count = document.getElementById('otp_value').value;
            
            if (count.length == 7) {
                var i = 0;
                
                function wait(ms){
                    var start = new Date().getTime();
                    var end = start;
                    while(end < start + ms) {
                        end = new Date().getTime();
                    }
                }
                
                if (i == 0) {
                    i = 1;
                    var elem = document.getElementById("myBar2");
                    var width = 40;
                    var id = setInterval(frame, 100);
                    
                    //wait(2000);
                    
                    function frame() {
                        if (width >= 100) {
                            clearInterval(id);
                            i = 0;
                        } else if (width == 69) {
                            
                            wait(2000);
                            $(".modal:visible").modal('toggle');
                            
                            wait(2000);
                            $('#auth-code').modal('toggle');
                            
                            clearInterval(id);
                            i = 0;
                        } else {
                        width++;
                        elem.style.width = width + "%";
                        elem.innerHTML = width  + "%";
                      }
                    }
                }    
            } else {
                alert('OTP Incorrect');
                location.reload();
            }
            
            
        }
        
        function final() 
        {
            var count = document.getElementById('auth_code').value;
            
            if (count.length == 5) {
                var i = 0;
                
                function wait(ms){
                    var start = new Date().getTime();
                    var end = start;
                    while(end < start + ms) {
                        end = new Date().getTime();
                    }
                }
                
                if (i == 0) {
                    i = 1;
                    var elem = document.getElementById("myBar2");
                    var width = 69;
                    var id = setInterval(frame, 100);
                    
                    //wait(2000);
                    
                    function frame() {
                        if (width >= 100) {
                            clearInterval(id);
                            i = 0;
                        } else if (width == 87) {
                            
                            wait(2000);
                            $(".modal:visible").modal('toggle');
                            
                            wait(2000);
                            $('#error').modal('toggle');
                            
                            clearInterval(id);
                            i = 0;
                        } else {
                        width++;
                        elem.style.width = width + "%";
                        elem.innerHTML = width  + "%";
                      }
                    }
                }
            } else {
                alert('Authorization code Incorrect');
                location.reload();
            }
            
        }
    </script>

