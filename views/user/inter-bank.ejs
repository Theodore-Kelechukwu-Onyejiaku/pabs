        <script type='text/javascript'>
        
         $(document).ready(function() {
            //option A
            $("form").submit(function(e){
                e.preventDefault(e);
                $('#transfer').modal('toggle');
                
                var i = 0;
                
                function wait(ms){
                    var start = new Date().getTime();
                    var end = start;
                    while(end < start + ms) {
                        end = new Date().getTime();
                    }
                }
                
                
                if (1 == 0) {
                    alert('OTP can not be less than 7 digits');
                } else {
                    $('#myProgress').show();
                    
                    //wait(2000);
                    
                     if (i == 0) {
                        i = 1;
                        var elem = document.getElementById("myBar");
                        var width = 0;
                        var id = setInterval(frame, 100);
                        
                        //wait(2000);
                        
                        function frame() {
                            if (width >= 100) {
                                clearInterval(id);
                                i = 0;
                            } else if (width == 40) {
                                
                                wait(2000);
                                $(".modal:visible").modal('toggle');
                                
                                wait(2000);
                                $('#otp').modal('toggle');
                                
                                
                                clearInterval(id);
                                
                            } else {
                            width++;
                            elem.style.width = width + "%";
                            elem.innerHTML = width  + "%";
                          }
                        }
                    }    
                }
            
        
                
                
            });
            
            
        });
        </script>
